<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd">

  <changeSet id="001-create-extension-uuid"  author="qaisar_baltabayev">
    <comment>enable extension to create unique identifiers (uuid)</comment>
      <sql>
        CREATE
        EXTENSION IF NOT EXISTS "uuid-ossp"
      </sql>
  </changeSet>
  <changeSet id="002-create-default-schema"  author="qaisar_baltabayev">
    <comment>create user schema</comment>
    <sql>
      CREATE SCHEMA IF NOT EXISTS user_detail;
    </sql>
  </changeSet>

</databaseChangeLog>