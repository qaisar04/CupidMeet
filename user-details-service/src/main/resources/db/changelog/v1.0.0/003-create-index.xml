<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd">

    <changeSet id="add-index-city-to-user-infos" author="qaisar_baltabayev">
        <createIndex tableName="user_infos" indexName="idx_user_infos_city">
            <column name="city"/>
        </createIndex>
    </changeSet>

    <changeSet id="add-index-age-to-user-infos" author="qaisar_baltabayev">
        <createIndex tableName="user_infos" indexName="idx_user_infos_age">
            <column name="age"/>
        </createIndex>
    </changeSet>

</databaseChangeLog>